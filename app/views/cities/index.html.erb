<div class="cities index">
  <h2>Cities</h2>

<table class="listing" summary="City list">
    <tr class="header">
      <th>City</th>
      <th>Short</th>
    </tr>
    <% @cities.each do |city| %>
    <tr>
      <td><%= city.cities_name %></td>
      <td><%= city.short_name %></td>
      </td>
    </tr>
    <% end %>
  </table>

<%= will_paginate @cities %>


</div>
